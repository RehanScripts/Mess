{
  "name": "smart-mess",
  "version": "1.0.0",
  "description": "Simple static site with a small Express+SQLite backend for login redirect demo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "health": "node -e \"require('http').get('http://localhost:'+(process.env.PORT||3000)+'/_health',res=>res.pipe(process.stdout))\"",
    "kill-ports": "bash -lc 'lsof -ti:3000-3010 | xargs -r kill -9'"
  },
